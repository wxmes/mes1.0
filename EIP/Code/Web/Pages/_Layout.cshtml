
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>EIP基础权限版</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <script src="https://cdn.bootcss.com/jquery/2.2.3/jquery.min.js" asp-append-version="true"></script>
    <link href="https://cdn.bootcss.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" asp-append-version="true">
    <link href="https://cdn.bootcss.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" asp-append-version="true">
    <link href="~/lib/plugins/layui/css/layui.css" asp-append-version="true" rel="stylesheet" />
    <environment names="Development">
        <link href="~/lib/dist/css/AdminLTE.css" rel="stylesheet" asp-append-version="true">
        <script src="~/lib/resource/language_zh.js" asp-append-version="true"></script>
    </environment>
    <environment names="Staging,Production">
        <link href="~/build/css/eip.adminlte.min.css" rel="stylesheet" asp-append-version="true" />
        <script src="~/build/js/eip.resource.min.js" asp-append-version="true"></script>
    </environment>
    <link href="https://cdn.bootcss.com/admin-lte/2.3.8/css/skins/_all-skins.min.css" rel="stylesheet" asp-append-version="true">
    <link href="https://cdn.bootcss.com/bootstrap-daterangepicker/2.1.27/daterangepicker.min.css" asp-append-version="true" rel="stylesheet">
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js" asp-append-version="true"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js" asp-append-version="true"></script>
    <![endif]-->
    <!--请求提示-->
    <link href="https://cdn.bootcss.com/bootstrap3-dialog/1.35.4/css/bootstrap-dialog.min.css" rel="stylesheet" asp-append-version="true">
    <link href="https://cdn.bootcss.com/pace/1.0.2/themes/black/pace-theme-loading-bar.min.css" id="pace-style" rel="stylesheet" asp-append-version="true">
    <link href="~/lib/plugins/jstree/dist/themes/default/style.min.css" rel="stylesheet" asp-append-version="true" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css" rel="stylesheet" asp-append-version="true">
    <link href="https://cdn.bootcss.com/iCheck/1.0.2/skins/all.css" rel="stylesheet" asp-append-version="true">
    <link href="https://cdn.bootcss.com/iCheck/1.0.2/skins/flat/blue.css" rel="stylesheet" asp-append-version="true">
    <script src="https://cdn.bootcss.com/pace/1.0.2/pace.min.js" asp-append-version="true"></script>
    <script src="https://cdn.bootcss.com/jqueryui/1.11.4/jquery-ui.min.js" asp-append-version="true"></script>
</head>
<body id="eip-body" class="sidebar-mini skin-navyblue pace-done fixed">
    <div class="eip-loading" id="eip-loading"></div>
    <div class="wrapper">
        @*顶部*@
        @Html.Partial("_MainHeader")
        @*导航菜单*@
        @Html.Partial("_MainSidebar")
        @*内容显示*@
        <div id="mainDiv" class="content-wrapper">
            @RenderBody()
            <section class="content" id="tab-page-content"></section>
        </div>
        @*页脚*@
        @Html.Partial("_MainFooter")
        @*右边弹出菜单栏*@
        @Html.Partial("_ControlSidebar")
    </div>
    <script src="https://cdn.bootcss.com/bootstrap/3.3.7/js/bootstrap.min.js" asp-append-version="true"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.js" asp-append-version="true"></script>
    <script src="https://cdn.bootcss.com/bootstrap-daterangepicker/2.1.27/moment.js" asp-append-version="true"></script>
    <script src="https://cdn.bootcss.com/bootstrap-daterangepicker/2.1.27/daterangepicker.min.js" asp-append-version="true"></script>
    <script src="https://cdn.bootcss.com/fastclick/1.0.6/fastclick.min.js" asp-append-version="true"></script>
    <script src="https://cdn.bootcss.com/jQuery-slimScroll/1.3.8/jquery.slimscroll.min.js" asp-append-version="true"></script>
    <script src="https://cdn.bootcss.com/iCheck/1.0.2/icheck.min.js" asp-append-version="true"></script>
    <script src="https://cdn.bootcss.com/jstree/3.3.5/jstree.min.js" asp-append-version="true"></script>
    <script src="https://cdn.bootcss.com/echarts/4.0.4/echarts.min.js" asp-append-version="true"></script>
    <script src="~/lib/plugins/validation/formValidation.min.js" asp-append-version="true"></script>
    <script src="~/lib/plugins/validation/framework/bootstrap.min.js" asp-append-version="true"></script>
    <script src="~/lib/plugins/validation/language/zh_CN.js" asp-append-version="true"></script>
    <script src="~/lib/plugins/validation/base-form.js" asp-append-version="true"></script>
    <script src="~/lib/plugins/pureClearButton/jquery.bootstrap-pureClearButton.min.js" asp-append-version="true"></script>
    <script src="~/lib/plugins/bootstrap-dialog/js/bootstrap-dialog.min.js" asp-append-version="true"></script>
    <script src="~/lib/plugins/bootstrap-dialog/js/base-modal.js" asp-append-version="true"></script>
    <script src="~/lib/plugins/layui/layui.js" asp-append-version="true"></script>
    @*开发系统*@
    <environment names="Development">
        <link href="~/css/extention/adminlte.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/css/extention/jstree.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/css/extention/layui.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/css/extention/map.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/css/extention/skin.css" rel="stylesheet" asp-append-version="true" />
        <link href="~/css/extention/modal.css" rel="stylesheet" asp-append-version="true" />
        <script src="~/lib/dist/js/app.js" asp-append-version="true"></script>
        <script src="~/app/console/home/index.js" asp-append-version="true"></script>
        <script src="~/lib/dist/js/sidebar-menu.js" asp-append-version="true"></script>
        <script src="~/lib/function.js" asp-append-version="true"></script>
    </environment>
    @*正式系统*@
    <environment names="Staging,Production">
        <script src="~/build/js/app.min.js" asp-append-version="true"></script>
        <script src="~/build/js/eip.min.js" asp-append-version="true"></script>
        <link href="~/build/css/eip.min.css" rel="stylesheet" asp-append-version="true" />
    </environment>
</body>
</html>