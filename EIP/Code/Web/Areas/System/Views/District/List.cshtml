@{
    Layout = "~/Views/Shared/_LayoutPageBaseList.cshtml";
}
<div class="row">
    <div class="col-md-2 padding-right-box">
        <div class="box box-widget">
            <div class="box-header with-border">
                <h3 class="box-title">行政区划</h3>
            </div>
            <div class="box-body">
                <ul id="tree" class="jstree"></ul>
            </div>
        </div>
    </div>
    <div class="col-md-10 padding-left-zero">
        <div class="box box-widget">
            <div class="box-header with-border">
                <h3 class="box-title">行政区划列表</h3>
                <div class="box-tools pull-right">
                    <div id="table-toolbar"></div>
                </div>
            </div>
            <div class="box-body">
                <table id="district-list"></table>
            </div>
        </div>
    </div>
</div>
<environment names="Development">
    <script src="~/app/system/district/list.js" asp-append-version="true"></script>
</environment>
<environment names="Staging,Production">
    <script src="~/build/js/system/district/list.min.js" asp-append-version="true"></script>
</environment>

@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers