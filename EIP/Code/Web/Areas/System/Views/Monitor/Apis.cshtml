@{
    Layout = "~/Views/Shared/_LayoutPageBaseList.cshtml";
}
<div class="row">
    <div class="col-md-12">
        <div class="box box-widget">
            <div class="box-header with-border">
                <h3 class="box-title">Api列表</h3>
                <div class="box-tools pull-right" id="table-toolbar">
                    <button onclick="getGridData();" title="刷新" data-toggle="tooltip" class="btn btn-flat btn-sm eip-btn"><i class="fa fa-refresh">刷新</i></button>
                </div>
            </div>
            <div class="box-body">
                <table id="list"></table>
            </div>
        </div>
    </div>
</div>
<environment names="Development">
    <script src="~/app/system/monitor/apis.js" asp-append-version="true"></script>
</environment>
<environment names="Staging,Production">
    <script src="~/build/js/system/monitor/apis.min.js" asp-append-version="true"></script>
</environment>

@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

